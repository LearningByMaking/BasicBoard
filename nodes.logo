to assignProjectsToNodes
make "nodes 
[
	20 light.prj 
	19 temp.prj
]     
end

to controlNode :n
talkto :n
make "srcfile projectForNode :n
compile
end

to projectForNode :n
make "i 0
repeat (count :nodes) / 2	
[
	make "current nth :i :nodes
	if (:current = :n) [output nth :i + 1 :nodes] 
	make "i :i + 2
]
end

